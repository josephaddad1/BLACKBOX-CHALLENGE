<!-- include header -->
<%- include('include/_header') %>
    <!-- /include header -->

    <!-- Main Site -->
    <main id="site-main">
        <div class="container">
            <div class="form-title text-center">
                <h2 class="text-dark">Enter below info to signup</h2>
            </div>
            <form id="sign_up">
                <div class="new_user">
                    <div class="form-group">
                        <label for="id" class="text-light">Username</label>
                        <input type="text" name="id" value="">
                    </div>
                    <div class="form-group">
                        <label for="password" class="text-light">Password</label>
                        <input type="password" name="password" value="">
                    </div>
                    <div class="form-group">
                        <button type="submit" class="btn text-dark update">Sign Up</button>
                    </div>
                </div> 
            </form>

        </div>
    </main>
    <!-- /Main Site -->

    <!-- include footer -->
    <%- include('include/_footer') %>
        <!-- /include footer -->